        }
  %]
  def apply[%=self.name%](sc: SparkContext, sqlCtx: SQLContext, [%=source.name%]_RDD: RDD[Array[AnyRef]]): Any = {
    val fileName = s"""${_defaultFS}[%=self.localFileName%]"""
