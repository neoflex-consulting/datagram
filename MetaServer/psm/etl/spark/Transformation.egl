    }: [%=f.getFullJavaClassName()%])([%for (i in f.sourceFields) {%]inputPort(/*[%=i.name%]*/[%=i.getIndex()%]).asInstanceOf[[%=i.getFullJavaClassName()%]][% if (hasMore){%],  [%}} %])[% if (hasMore){%],[%}%]
    
