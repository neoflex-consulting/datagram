      [%}%]            
      .load(path)
      [%for (explodeField in self.explodeFields) {%]
      .withColumn("""[%=explodeField.`alias`%]""",  explode($"""[%=explodeField.field%]"""))
      [%}%]            
      .map(row => {
          Array[AnyRef](
          [%for (field in self.outputPort.fields) {%]
          /*[%=field.name%]=*/ [%=field.parquetToJava("row", field.name)%][%if (hasMore){%],[%}%]
          
          [%}%]
          )
        })
