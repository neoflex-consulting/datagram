              <details key="documentation" value="This attribute holds the minimum length of a variable length segment."/>
            </eAnnotations>
          </eStructuralFeatures>
          <eStructuralFeatures xsi:type="ecore:EAttribute" name="rules" eType="#//cwmx/resource/imsdatabase/imstypes/RulesType">
