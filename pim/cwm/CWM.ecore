              <details key="documentation" value="This attribute holds the length of a fixed-length segment, or the maximum length of a variable length segment."/>
            </eAnnotations>
          </eStructuralFeatures>
          <eStructuralFeatures xsi:type="ecore:EAttribute" name="minimumLength" eType="#//cwm/objectmodel/core/Integer">
