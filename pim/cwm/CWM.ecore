              <details key="documentation" value="This attributes holds the value that indicates where to place new occurrences of this segment type in the physical database."/>
            </eAnnotations>
          </eStructuralFeatures>
          <eStructuralFeatures xsi:type="ecore:EAttribute" name="subsetPointers" eType="#//cwm/objectmodel/core/String">
            <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
              <details key="documentation" value="This attribute holds the number of subset pointers in a direct dependent segment in a DEDB DBD. Valid values are 0-8."/>
            </eAnnotations>
          </eStructuralFeatures>
          <eStructuralFeatures xsi:type="ecore:EAttribute" name="directDependent"
              eType="#//cwm/objectmodel/core/Boolean">
            <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
              <details key="documentation" value="This attribute indicates whether the segment is direct dependent or sequential. A value of TRUE specifies use of DIR as the segment type on the generated DBD. A value of FALSE specifies use of SEQ on the generated DBD. This attribute is ignored for the root segment of the DBD user object."/>
            </eAnnotations>
          </eStructuralFeatures>
          <eStructuralFeatures xsi:type="ecore:EAttribute" name="pcPointer" eType="#//cwmx/resource/imsdatabase/imstypes/ChildPointerType">
            <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
              <details key="documentation" value="This attribute describes the type of physical child pointer to be stored in the prefix area of the segment in the DBD. Valid Values are SNGL, DBLE, null"/>
            </eAnnotations>
          </eStructuralFeatures>
          <eStructuralFeatures xsi:type="ecore:EReference" name="logical" upperBound="-1"
              eType="#//cwmx/resource/imsdatabase/SegmentLogical" eOpposite="#//cwmx/resource/imsdatabase/SegmentLogical/physical">
            <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
              <details key="documentation" value="The logical segment that is based on this physical segment."/>
