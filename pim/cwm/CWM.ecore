          <eStructuralFeatures xsi:type="ecore:EReference" name="segment" eType="#//cwmx/analysis/informationset/Segment"
              eOpposite="#//cwmx/analysis/informationset/Segment/rule">
            <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
              <details key="documentation" value="The Segment for a Rule"/>
            </eAnnotations>
          </eStructuralFeatures>
          <eStructuralFeatures xsi:type="ecore:EReference" name="segmentRegion" eType="#//cwmx/analysis/informationset/SegmentRegion"
              eOpposite="#//cwmx/analysis/informationset/SegmentRegion/rule">
            <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
              <details key="documentation" value="The SegmentRegion for a Rule."/>
            </eAnnotations>
          </eStructuralFeatures>
        </eClassifiers>
