          </eStructuralFeatures>
          <eStructuralFeatures xsi:type="ecore:EReference" name="reportQuery" upperBound="-1"
              eType="#//cwm/foundation/datatypes/QueryExpression" eOpposite="#//cwm/foundation/datatypes/QueryExpression/reportGroup">
