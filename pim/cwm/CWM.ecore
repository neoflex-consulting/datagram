            <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
              <details key="documentation" value="The relationship to the logical children relationships."/>
            </eAnnotations>
          </eStructuralFeatures>
          <eStructuralFeatures xsi:type="ecore:EReference" name="sourcedIndex" upperBound="-1"
              eType="#//cwmx/resource/imsdatabase/SecondaryIndex" eOpposite="#//cwmx/resource/imsdatabase/SecondaryIndex/indexSource">
            <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
              <details key="documentation" value="The index that uses fields in this segment."/>
            </eAnnotations>
          </eStructuralFeatures>
          <eStructuralFeatures xsi:type="ecore:EReference" name="lparent" eType="#//cwmx/resource/imsdatabase/LCHILD"
