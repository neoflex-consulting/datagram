              <details key="documentation" value="This is used to arrange the segments in a partitioned database in a manner comparable to arranging them within datasets in earlier versions of IMS"/>
            </eAnnotations>
          </eStructuralFeatures>
          <eStructuralFeatures xsi:type="ecore:EReference" name="secondaryIndex" upperBound="32"
              eType="#//cwmx/resource/imsdatabase/SecondaryIndex" containment="true"
              eOpposite="#//cwmx/resource/imsdatabase/SecondaryIndex/segment">
            <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
              <details key="documentation" value="The secondary index relationships that would be represented as LCHILD/XDFLD statement sets."/>
