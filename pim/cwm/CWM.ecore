            </eAnnotations>
          </eStructuralFeatures>
          <eStructuralFeatures xsi:type="ecore:EAttribute" name="defaultCollationName"
              eType="#//cwm/objectmodel/core/String">
            <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
