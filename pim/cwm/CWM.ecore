          <details key="documentation" value="The version of the interchange pattern."/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="uri" eType="#//cwm/objectmodel/core/String">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="A URI identifying a human-readable pattern specfication document that describes the interchange pattern."/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="classification" eType="#//cwm/objectmodel/core/String">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="Identifies the structural classification of the pattern: Micro Pattern, Domain Pattern, Macro Pattern."/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="category" eType="#//cwm/objectmodel/core/String">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="Identifies the usage category of the pattern: Mapping, Typing, Extension, Interpretation, Generation, Structural."/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EReference" name="projection" eType="#//cwmmip/Projection"
          containment="true" resolveProxies="false">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="An object-valued array of type Projection that describes the metamodel graph subset defining the semantic context of the pattern. If this attribute contains more than one value, then the complete projection of the pattern is the graph union of multiple projection object values. This attribute has a multiplicity of 1..*."/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EReference" name="unitOfInterchange" upperBound="-1"
          eType="#//cwmmip/UnitOfInterchange" eOpposite="#//cwmmip/UnitOfInterchange/interchangePattern"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="componentPattern" upperBound="-1"
          eType="#//cwmmip/InterchangePattern" eOpposite="#//cwmmip/InterchangePattern/compositePattern"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="compositePattern" upperBound="-1"
          eType="#//cwmmip/InterchangePattern" eOpposite="#//cwmmip/InterchangePattern/componentPattern"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="ModeledSemanticContext" eSuperTypes="#//cwmmip/SemanticContext">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="ModeledSemanticContext is a subclass of SemanticContext that adds references to instances of MOF ModelElements and MOF Associations, as well as an association to MOF ModelElements as a means of specifying anchor classes. Constraints may be specified within ModelSemanticContext via the inherited SemanticContext::constraint attribute.&#xA;&#xA;Note that this class generally supports the construction of \&quot;explicitly modeled\&quot; pattern definitions; that is, pattern definitions composed of MOF metaclass instances (or MOF metaobjects), rather than simply logical class names."/>
      </eAnnotations>
      <eStructuralFeatures xsi:type="ecore:EReference" name="mofAssociation" upperBound="-1"
          eType="#//mof/model/Association" eOpposite="#//mof/model/Association/modeledProjection"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="mofElement" upperBound="-1"
          eType="#//mof/model/ModelElement" eOpposite="#//mof/model/ModelElement/modeledProjection"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="mofAnchorElement" upperBound="-1"
          eType="#//mof/model/ModelElement" eOpposite="#//mof/model/ModelElement/modeledSemanticContext"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Projection" abstract="true" eSuperTypes="#//cwm/objectmodel/core/Element">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Projection is an abstract base class that models the semantic context of the pattern, where by semantic context we mean a well-defined subset of the CWM metamodel graph."/>
      </eAnnotations>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="SemanticContext" eSuperTypes="#//cwmmip/Projection">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="SemaniticContext is a concrete subclass of Projection that specifies a projection by naming the CWM model elements and associations comprising the projection, along with any constraints on those elements."/>
      </eAnnotations>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="element" eType="#//cwm/objectmodel/core/String">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="Element contains the names of the CWM metamodel classes comprising the Projection, expressed as string-based, logical names. This attribute has a multiplicity of 0..*. These logical names may refer to elements from multiple CWM metamodel packages."/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="association" eType="#//cwm/objectmodel/core/String">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="Association contains the names of CWM associations comprising the Projection, expressed as string-based logical names. This attribute has a multiplicity of 0..*. These logical names may refer to associations from any number of CWM packages, including associations that span packages."/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EReference" name="constraint" eType="#//cwmmip/PatternConstraint"
          containment="true" resolveProxies="false">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="Constraint is a collection of pattern constraints that apply to elements and associations comprising the Projection. This attribute has a multiplicity of 0..*."/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="anchorElement" eType="#//cwm/objectmodel/core/String">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="AnchorElement names CWM metamodel elements from the Projection that have some distinguished meaning. Generally, these elements are thought of as root objects of the metamodel Projection graph and are used as starting points for navigating the graph. This attribute has a multiplicity of 0..*. Although a Projection may have multiple anchor points, in practice, most Projections will usually have a single anchor point."/>
        </eAnnotations>
      </eStructuralFeatures>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Element"/>
    <eClassifiers xsi:type="ecore:EClass" name="GraphSubset" eSuperTypes="#//cwmmip/Projection">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="GraphSubset is a concrete subclass of Projection that allows a projection to be specified by an expression which, when evaluated, describes some physical sub-graph of the CWM UML model."/>
      </eAnnotations>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="element" eType="#//cwm/objectmodel/core/String">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="Element is the logical name of a CWM metamodel element (usually a package or class) comprising the physical projection."/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="deepCopy" eType="#//cwm/objectmodel/core/Boolean">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="DeepCopy is a Boolean attribute which, when true, implies that all connected elements and their attributes, within the boundaries of the specified package, are to be included in the pattern projection."/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="copyDepth" eType="#//cwm/objectmodel/core/Integer">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="CopyDepth is an integer value that specifies the number of graph edges to traverse when establishing the physical graph projection, in cases when deepCopy is false."/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="aggregationsOnly" eType="#//cwm/objectmodel/core/Boolean">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="AggregationsOnly is a Boolean attribute which, when true, specifies that only composite elements and their components are to be included in the physical graph projection."/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="includeAssociations"
          eType="#//cwm/objectmodel/core/Boolean">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="IncludeAssociations is a Boolean attribute which, when true, specifies that associations are to be included in the physical graph projection."/>
        </eAnnotations>
      </eStructuralFeatures>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="PatternConstraint" eSuperTypes="#//cwm/objectmodel/core/Element">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="PatternConstraint is used to define constraints against the metamodel Projection. There is no requirement to use any particular constraint language. However, if OCL is used, the OCL expression contained by PatternConstraint::body must be a valid OCL expression relative to the modeling context of the Projection."/>
      </eAnnotations>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="body" eType="#//cwm/objectmodel/core/String">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="Constraint expressed as a textual string in some constraint language."/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="language" eType="#//cwm/objectmodel/core/String">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="documentation" value="Specfies the names of the constraint language used."/>
        </eAnnotations>
      </eStructuralFeatures>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="ModeledGraphSubset" eSuperTypes="#//cwmmip/GraphSubset">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="ModeledGraphSubset is a subclass of GraphSubset that adds a reference to a single instance of MOF ModelElement.&#xA;&#xA;Note that this class generally supports the construction of \&quot;explicitly modeled\&quot; pattern definitions; that is, pattern definitions composed of MOF metaclass instances (or MOF metaobjects), rather than simply logical class names."/>
      </eAnnotations>
      <eStructuralFeatures xsi:type="ecore:EReference" name="mofElement" eType="#//mof/model/ModelElement"
          eOpposite="#//mof/model/ModelElement/modeledGraphSubset"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Restriction" eSuperTypes="#//cwmmip/PatternConstraint">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="Restriction is a subclass of PatternConstraint that imposes structural constraints on an instance of a Projection. The resulting structure is then regarded as being invariant with regard to other values that might be supplied in the construction of the pattern instance (e.g., values of certain class attributes)."/>
      </eAnnotations>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="BindingParameter" eSuperTypes="#//cwmmip/PatternConstraint">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="BindingParameter is a subclass of PatternConstraint that typically asserts values (or value ranges) for class attributes. BindingParameters do not influence structure, but influence content instead."/>
      </eAnnotations>
    </eClassifiers>
  </eSubpackages>
  <eSubpackages name="mof" nsURI="http:///orgomg/mof.ecore" nsPrefix="orgomg.mof">
    <eSubpackages name="model" nsURI="http:///orgomg/mof/model.ecore" nsPrefix="orgomg.mof.model">
      <eClassifiers xsi:type="ecore:EClass" name="ModelElement">
        <eStructuralFeatures xsi:type="ecore:EReference" name="modeledGraphSubset"
            upperBound="-1" eType="#//cwmmip/ModeledGraphSubset" eOpposite="#//cwmmip/ModeledGraphSubset/mofElement"/>
        <eStructuralFeatures xsi:type="ecore:EReference" name="modeledProjection"
            upperBound="-1" eType="#//cwmmip/ModeledSemanticContext" eOpposite="#//cwmmip/ModeledSemanticContext/mofElement"/>
        <eStructuralFeatures xsi:type="ecore:EReference" name="modeledSemanticContext"
            upperBound="-1" eType="#//cwmmip/ModeledSemanticContext" eOpposite="#//cwmmip/ModeledSemanticContext/mofAnchorElement"/>
      </eClassifiers>
      <eClassifiers xsi:type="ecore:EClass" name="Association">
        <eStructuralFeatures xsi:type="ecore:EReference" name="modeledProjection"
            upperBound="-1" eType="#//cwmmip/ModeledSemanticContext" eOpposite="#//cwmmip/ModeledSemanticContext/mofAssociation"/>
      </eClassifiers>
    </eSubpackages>
  </eSubpackages>
</ecore:EPackage>
