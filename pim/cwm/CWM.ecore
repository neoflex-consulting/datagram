          <eStructuralFeatures xsi:type="ecore:EReference" name="dbd" lowerBound="1"
              eType="#//cwmx/resource/imsdatabase/DBD" transient="true" eOpposite="#//cwmx/resource/imsdatabase/DBD/segment">
            <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
              <details key="documentation" value="DBD that owns this segment."/>
            </eAnnotations>
