          <eStructuralFeatures xsi:type="ecore:EReference" name="senSegment" lowerBound="1"
              eType="#//cwmx/resource/imsdatabase/SenSegment" transient="true" eOpposite="#//cwmx/resource/imsdatabase/SenSegment/senField">
            <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
              <details key="documentation" value="The segment that is sensitive to this field."/>
            </eAnnotations>
          </eStructuralFeatures>
