          <eStructuralFeatures xsi:type="ecore:EReference" name="senseg" upperBound="-1"
              eType="#//cwmx/resource/imsdatabase/SenSegment" eOpposite="#//cwmx/resource/imsdatabase/SenSegment/segment">
            <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
              <details key="documentation" value="The sensitive segments that use this segment."/>
            </eAnnotations>
          </eStructuralFeatures>
          <eStructuralFeatures xsi:type="ecore:EReference" name="child" upperBound="-1"
              eType="#//cwmx/resource/imsdatabase/Segment" eOpposite="#//cwmx/resource/imsdatabase/Segment/parent">
            <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
              <details key="documentation" value="The physical child segment."/>
            </eAnnotations>
          </eStructuralFeatures>
          <eStructuralFeatures xsi:type="ecore:EReference" name="parent" eType="#//cwmx/resource/imsdatabase/Segment"
              eOpposite="#//cwmx/resource/imsdatabase/Segment/child">
            <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
              <details key="documentation" value="The physical parent segment."/>
