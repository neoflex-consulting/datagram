            <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
              <details key="documentation" value="Identifies the DataItem instances that participate in the Set instance?s key data."/>
            </eAnnotations>
          </eStructuralFeatures>
          <eStructuralFeatures xsi:type="ecore:EReference" name="partitionedSet" upperBound="-1"
              eType="#//cwmx/resource/dmsii/Set" eOpposite="#//cwmx/resource/dmsii/Set/partitioningSet">
            <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
              <details key="documentation" value="Identifies the partitioned set."/>
            </eAnnotations>
          </eStructuralFeatures>
          <eStructuralFeatures xsi:type="ecore:EReference" name="partitioningSet"
              eType="#//cwmx/resource/dmsii/Set" eOpposite="#//cwmx/resource/dmsii/Set/partitionedSet">
            <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
              <details key="documentation" value="Identifies the partitioning set."/>
            </eAnnotations>
          </eStructuralFeatures>
          <eStructuralFeatures xsi:type="ecore:EReference" name="partitionedDataSet"
              eType="#//cwmx/resource/dmsii/DataSet" eOpposite="#//cwmx/resource/dmsii/DataSet/partitioningSet">
